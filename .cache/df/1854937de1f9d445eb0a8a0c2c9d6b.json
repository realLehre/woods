\r\n\r\n        &:first-child:after {\r\n          position: absolute;\r\n          content: '';\r\n          bottom: -2px;\r\n          left: 50%;\r\n          transform: translateX(-50%);\r\n          width: 75%;\r\n          height: 2px;\r\n          background: c.$dark-color;\r\n        }\r\n\r\n        &:before {\r\n          position: absolute;\r\n          content: '';\r\n          bottom: -2px;\r\n          left: 50%;\r\n          transform: translateX(-50%);\r\n          width: 0;\r\n          height: 2px;\r\n          background: c.$dark-color;\r\n          @include c.transition;\r\n        }\r\n        &:hover::before {\r\n          width: 75%;\r\n        }\r\n      }\r\n    }\r\n\r\n    & #nav-actions {\r\n      @include c.flex(row);\r\n      & i {\r\n        cursor: pointer;\r\n        @include c.transition;\r\n\r\n        &:hover {\r\n          color: c.$dark-milk;\r\n        }\r\n      }\r\n\r\n      & i:nth-child(2) {\r\n        margin-left: 15px;\r\n      }\r\n\r\n      & .nav-toggler {\r\n        width: 30px;\r\n        height: 25px;\r\n        cursor: pointer;\r\n        @include c.flex(column);\r\n        display: none;\r\n\r\n        &.open .bar {\r\n          transform: rotate(-42deg);\r\n        }\r\n        &.open .start {\r\n          transform: rotate(-500deg) translate(-0px, -8px);\r\n        }\r\n        &.open .end {\r\n          transform: rotate(-500deg) translate(0px, 8px);\r\n        }\r\n\r\n        & .bar {\r\n          width: 100%;\r\n          height: 3px;\r\n          border-radius: 3px;\r\n          background: c.$dark-color;\r\n          transition: all 500ms cubic-bezier(0.37, 0.1, 1, 0.5);\r\n\r\n          &:nth-child(odd) {\r\n            width: 60%;\r\n          }\r\n\r\n          &.start {\r\n            align-self: flex-start;\r\n            transform-origin: right;\r\n          }\r\n          &.end {\r\n            align-self: flex-end;\r\n            transform-origin: left;\r\n          }\r\n        }\r\n      }\r\n   