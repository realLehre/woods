: 0;\r\n  box-sizing: border-box;\r\n  // outline: 1px solid red;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n  color: $dark-color;\r\n}\r\n\r\nli {\r\n  list-style: none;\r\n}\r\n\r\n@mixin flex($direction) {\r\n  display: flex;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  flex-direction: $direction;\r\n}\r\n\r\n@mixin transition {\r\n  transition: all 700ms ease-in-out;\r\n}\r\n\r\n@mixin tablets {\r\n  @media screen and (max-width: 768px) {\r\n    @content;\r\n  }\r\n}\r\n\r\n@mixin mobiles {\r\n  @media screen and (max-width: 500px) {\r\n    @content;\r\n  }\r\n}\r\n\r\n@mixin small-mobiles {\r\n  @media screen and (max-width: 380px) {\r\n    @content;\r\n  }\r\n}\r\n","src\\scss\\_screen.scss":"@use 'config' as c;\r\n\r\n@mixin tablets {\r\n  @media screen and (max-width: 768px) {\r\n    .header {\r\n      & .header-items {\r\n        & #nav-list {\r\n          @include c.flex(row);\r\n          width: 300px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@mixin mobiles {\r\n  @media screen and (max-width: 500px) {\r\n    .header {\r\n      & .header-items {\r\n        & .blocks {\r\n          position: absolute;\r\n          top: 0;\r\n          left: 0;\r\n          width: 100%;\r\n          height: 100%;\r\n          z-index: 50;\r\n          @include c.flex(row);\r\n\r\n          & .block {\r\n            height: 100%;\r\n            width: 100px;\r\n            background: c.$dark-milk;\r\n            transform: translateY(-100%);\r\n            @include c.transition;\r\n            transition: all 500ms cubic-bezier(0.3, 0.1, 1, 0.5);\r\n\r\n            &.drop {\r\n              transform: translateY(0);\r\n            }\r\n          }\r\n        }\r\n\r\n        & #nav-list {\r\n          position: absolute;\r\n          background: transparent;\r\n          top: 0;\r\n          left: 0;\r\n          width: 100%;\r\n          height: 100%;\r\n          z-index: 70;\r\n          @include c.flex(column);\r\n          transform: translateY(-100%);\r\n          @include c.transition;\r\n          align-items: center;\r\n          justify-content: center;\r\n          transition-delay: 0.4s;\r\n          overflow: hidden;\r\n\r\n          &.open {\r\n            transform: translateY(0);\r\n          }\r\n\r\n          & .nav-item {\r\n            font-size: 30px;\r\n            // transform: translateX(1000%);\r\n            @include c.transition;\r\n\r\n            &:nth-child(even) {\r\n              transform: translateX(-400%);\r\n              &.open {\r\n                transform: translateX(-30px);\r\n              }\r\n            }\r\n            &:nth-child(odd) {\r\n              transform: translateX(400%);\r\n              &.open {\r\n                transform: translateX(30px);\r\n              }\r\n            }\r\n\r\n            &:not(:last-child) {\r\n              margin-bottom: 60px;\r\n            }\r\n          }\r\n        }\r\n\r\n        & #nav-actions {\r\n          @include c.flex(row);\r\n          & i {\r\n            cursor: pointer;\r\n            @include c.transition;\r\n\r\n            &:hover {\r\n              color: c.$dark-milk;\r\n            }\r\n          }\r\n\r\n          & i:nth-child(2) {\r\n            margin-left: 10px;\r\n          }\r\n\r\n          & .nav-toggler {\r\n            width: 30px;\r\n            height: 25px;\r\n            cursor: pointer;\r\n            @include c.flex(column);\r\n            margin-left: 20px;\r\n            z-index: 100;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@for $i from 1 through 5 {\r\n  .header {\r\n    & .header-items {\r\n      & .blocks {\r\n        & .block:nth-child(#{$i}) {\r\n          transition-delay: ($i * 0.2s);\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n","src\\scss\\main.scss":"@use 'config' as c;\r\n@use 'screen' as s;\r\n\r\nbody {\r\n  background: white;\r\n  font-family: 'Nunito', sans-serif;\r\n}\r\n\r\n.header {\r\n  font-family: 'Plus Jakarta Display', sans-serif;\r\n  background: c.$sec-color;\r\n  padding: 15px 0;\r\n\r\n  & .header-items {\r\n    @include c.flex(row);\r\n    max-width: 1400px;\r\n    margin: 0 auto;\r\n    color: c.$dark-color;\r\n\r\n    & #logo {\r\n      font-size: map-get(c.$font-sizes, main-contents);\r\n      font-weight: map-get(c.$font-weight, bold);\r\n      cursor: pointer;\r\n      @include c.transition;\r\n\r\n      &:hover {\r\n        color: c.$dark-milk;\r\n      }\r\n    }\r\n\r\n    & .blocks {\r\n      display: none;\r\n    }\r\n\r\n    & #nav-list {\r\n      @include c.flex(row);\r\n      width: 400px;\r\n\r\n      & .nav-item {\r\n        position: relative;\r\n        font-size: map-get(c.$font-sizes, nav-footer-texts);\r\n\r\n        &:first-child:after {\r\n          position: absolute;\r\n          content: '';\r\n          bottom: -2px;\r\n          left: 50%;\r\n          transform: translateX(-50%);\r\n          width: 75%;\r\n          height: 2px;\r\n          background: c.$dark-color;\r\n        }\r\n\r\n        &:before {\r\n          position: absolute;\r\n          content: '';\r\n          bottom: -2px;\r\n          left: 50%;\r\n          transform: translateX(-50%);\r\n          width: 0;\r\n          height: 2px;\r\n          background: c.$dark-color;\r\n          @include c.transition;\r\n        }\r\n        &:hover::before {\r\n          width: 75%;\r\n        }\r\n      }\r\n    }\r\n\r\n    & #nav-actions {\r\n      @include c.flex(row);\r\n      & i {\r\n        cursor: pointer;\r\n        @include c.transition;\r\n\r\n        &:hover {\r\n          color: c.$dark-milk;\r\n        }\r\n      }\r\n\r\n      & i:nth-child(2) {\r\n        margin-left: 15px;\r\n      }\r\n\r\n      & .nav-toggler {\r\n        width: 30px;\r\n        height: 25px;\r\n        cursor: pointer;\r\n        @include c.flex(column);\r\n        display: none;\r\n\r\n        &.open .bar {\r\n          transform: rotate(-42deg);\r\n        }\r\n        &.open .start {\r\n          transform: rotate(-3198deg) translate(19px, 8px);\r\n        }\r\n        &.open .end {\r\n          // transform: rotate(-500deg) translate(0px, 8px);\r\n          transform: rotate(-3198deg) translate(-18px, -8px);\r\n        }\r\n\r\n        & .bar {\r\n          width: 100%;\r\n          height: 3px;\r\n          border-radius: 3px;\r\n          background: c.$dark-color;\r\n          transition: all 1550ms cubic-bezier(0.37, 0.5, 1, 0.5);\r\n\r\n          &:nth-child(odd) {\r\n            width: 60%;\r\n          }\r\n\r\n          &.start {\r\n            align-self: flex-start;\r\n            transform-origin: right;\r\n          }\r\n          &.end {\r\n            align-self: flex-end;\r\n            transform-origin: left;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@include s.tablets;\r\n@include s.mobiles;\r\n"},"lineCount":null}},"error":null,"hash":"d667bf191c80b79a3afc6aefaf4ac7be","cacheData":{"env":{}}}